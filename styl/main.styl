@-webkit-keyframes slide1 {
  0% {
    -webkit-transform: translate(0px,0px);
    -webkit-animation-fill-mode: forwards;
  }
  100% {
    -webkit-transform: translate(-1045px,0px);
    -webkit-animation-fill-mode: forwards;
  }
}

@-webkit-keyframes slide2 {
  0% {
      -webkit-transform: translate(-1420px,0px) scale(1.20,1.20);
    }
  100% {
    -webkit-transform: translate(0px,-541px) scale(1,1);
  }
}

