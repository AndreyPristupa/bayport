#content
  width 1080px
  height 1920px
  z-index 1
  margin: auto
  position: relative
  overflow hidden
  .bg
    position: absolute;
    width: 2125px
    height: 1920px
    left: 0;
    z-index: -2
    right: 0;
    top: 0;
    bottom: 0;
    //background-image: url(../images/bg_slides_new/1.jpg);
    //background-size: 100% 100%, 0% 100%;
    //z-index: -1;
    ////-webkit-transition-duration: 1s;
