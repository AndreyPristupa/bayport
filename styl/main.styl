#town-collection-perview
  position: absolute
  img
    opacity .2
  .town-collection-block
    background-image: url(../images/Products/Category/TH/townhomesblock.jpg)
  .town-collection-block2
    background-image: url(../images/Products/Category/TH/townhomesblock2.jpg)
  .town-collection-block, .town-collection-block2
    background-attachment: fixed;
    background-position: top;
    -webkit-transition 1s
    width: 277px;
    left: 236px;
    height: 286px;
    position: absolute;
    top: 0;
    &:after
      position: absolute;
      content: ' ';
      width: 100%
      height: 100%;
      top: 0;
      left: 0;
      opacity: .8
      background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(255,255,255,1)), color-stop(100%,rgba(255,255,255,0))); /* Chrome,Safari4+ */
      background: -webkit-linear-gradient(left, rgba(255,255,255,1) 0%,rgba(255,255,255,0) 24%); /* Chrome10+,Safari5.1+ */
    &:before
      position: absolute;
      content: ' ';
      width: 100%
      height: 100%;
      top: 0;
      opacity .8
      right: 0;
      background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(255,255,255,0)), color-stop(100%,rgba(255,255,255,1))); /* Chrome,Safari4+ */
      background: -webkit-linear-gradient(left, rgba(255,255,255,0) 75%,rgba(255,255,255,1) 100%); /* Chrome10+,Safari5.1+ */

