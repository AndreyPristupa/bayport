    //-webkit-animation-duration: 4s;
    //-webkit-animation-timing-function: linear;
    //-webkit-animation-iteration-count: infinite;
  #index-title
    display: block
    position: absolute
    top: 200px
    color:#fff;
    h3, h2
      display: block
      font-weight: normal
      line-height: 1
    h3
      transition 1s
      font-size: 50px
      margin: 0 0 0 250px
    h2
      font-size: 132px
      margin: 0 0 0 100px
      font-family: Gabriola
      width: 400px
  .clickable
    cursor pointer
  .menu-wrapper
    position: absolute
    top: 1300px
    #amenities-menu
      left: 294px
    #site-menu
      left: 312px
    #design-menu
      left 97px
    #design-menu, #amenities-menu, #site-menu
      position: absolute
      z-index -1
      font-size: 20px
      top:0
      transition top 600ms ease
      &.active
        top -72px
      ul
        list-style none
        li
          display: inline-block
          border-top 1px solid #90c188
          border-left 1px solid #90c188
          border-right 1px solid #90c188
          border-top-left-radius 20px
          border-top-right-radius 20px
          padding 11px 76px
          background-color #fff
          color:#004f8c
          &:hover, &.active
            background-color #90c188
            color:#fff
    #active-menu
      color #fff
      font-size: 30px
      position: absolute;
      top: 21px;
      left: 402px;
      ul
        list-style none
        li
          display: inline-block;
          border: 7px solid #aacea0;
          padding: 16px 22px;
          margin-right: 10px;
          opacity: 0;
          transition: 0.5s;
          &:hover
            background-color #fff
            color #004f8c
          &.active
            background-color #fff
            color #004f8c
    &.index-menu
      height: 350px
      transition 1.3s
      background-color: #000
      opacity: .8
      width: 1080px
      &.menu-active
        height: 520px
        top 1500px
        background-color:#004f8c
        opacity: 1
        .index-menu-title
          opacity 0
      & #menu-logo
        padding: 65px 0 0 33px
        transition 2s
  #menu-logo.menu-logo-active
    padding: 130px 0 0 33px !important
  .index-menu-title
    transition 1.1s
  .inline-block
